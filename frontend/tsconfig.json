{
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@api/*": ["src/api/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@pages/*": ["src/pages/*"],
      "@types/*": ["src/types/*"],
  }
},
  "extends": "expo/tsconfig.base"
}
